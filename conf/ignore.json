{
  ".gitattributes": [
    "# 非特殊标示的都识别成文本文件,行结束符为lf",
    "* text       eol=lf",
    "*.multipart  -text",
    "*.bat        -text",
    "*.png        binary",
    "*.ico        binary",
    "*.gif        binary",
    "*.jar        binary",
    "*.jpeg       binary",
    "*.jpg        binary",
    "*.vsd        binary"
  ],
  ".gitignore": [
    ".temp_cache",
    ".idea",
    ".vscode",
    ".DS_Store",
    ".swc",
    "/coverage",
    "/dist",
    "/docs",
    "/es",
    "/lib",
    "__snapshots__",
    "node_modules",
    "package-lock.json",
    "yarn.lock"
  ],
  ".eslintrc.yaml": ["extends:", "  - neko"],
  ".prettierrc.yaml": ["printWidth: 100", "singleQuote: true", "bracketSameLine: false"],
  ".prettierignore": ["LICENSE", ".gitignore", ".DS_Store", ".editorconfig", ".eslintignore"],
  ".stylelintrc.yaml": [
    "extends:",
    "  - stylelint-config-moneko",
    "ignoreFiles:",
    "  - coverage/**/*"
  ],
  ".commitlintrc.yaml": [
    "extends:",
    "  - \"@commitlint/config-conventional\"",
    "formatter: \"@commitlint/format\"",
    "helpUrl: https://github.com/conventional-changelog/commitlint/#what-is-commitlint"
  ]
}
