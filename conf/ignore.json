{
  ".gitattributes": [
    "# 非特殊标示的都识别成文本文件,行结束符为lf",
    "* text       eol=lf",
    "*.multipart  -text",
    "*.bat        -text",
    "*.png        binary",
    "*.ico        binary",
    "*.gif        binary",
    "*.jar        binary",
    "*.jpeg       binary",
    "*.jpg        binary",
    "*.vsd        binary"
  ],
  ".gitignore": [
    ".temp_cache",
    ".idea",
    ".vscode",
    ".DS_Store",
    "node_modules",
    "node_modules.nosync",
    "/dist",
    "/docs",
    "/lib",
    "/coverage",
    "__snapshots__",
    "/es",
    "package-lock.json"
  ],
  ".eslintrc.yaml": [
    "extends:",
    "  - neko",
    "rules:",
    "  import/no-unresolved:",
    "    - 2",
    "    - ignore:",
    "      - \\?raw$",
    "      - ^@pkg",
    "      - ^@/",
    "      - components",
    "      - ^libraryNameTemplate"
  ],
  ".prettierrc.yaml": [
    "printWidth: 100",
    "singleQuote: true",
    "bracketSameLine: false"
  ],
  ".prettierignore": [
    "LICENSE",
    ".gitignore",
    ".DS_Store",
    ".editorconfig",
    ".eslintignore"
  ],
  ".stylelintrc.yaml": [
    "extends:",
    "  - stylelint-config-moneko",
    "ignoreFiles:",
    "  - coverage/**/*"
  ]
}
